@article{Howison:2015ja,
author = {Howison, James and Bullard, Julia},
title = {{Software in the scientific literature: Problems with seeing, finding, and using software mentioned in the biology literature}},
journal = {J. Assoc. Inf. Sci. Technol.},
year = {2016},
volume = {67},
number = {9},
pages = {2137--2155},
month = sep,
doi = {10.1002/asi.23538},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-01-13T04:54:03GMT},
date-modified = {2016-08-31T06:56:51GMT},
abstract = {Software is increasingly crucial to scholarship, yet the visibility and usefulness of software in the scientific record are in question. Just as with data, the visibility of software in publications is related to incentives to share software in reusable ways, and so promote efficient science. In this article, we examine software in publications through content analysis of a random sample of 90 biology articles. We develop a coding scheme to identify software {\textquotedblleft}mentions{\textquotedblright} and classify them according to their characteristics and ability to realize the functions of citations. Overall, we find diverse and problematic practices: Only between 31% and 43% of mentions involve formal citations; informal mentions are very common, even in high impact factor journals and across different kinds of software. Software is frequently inaccessible (15%{\textendash}29% of packages in any form; between 90% and 98% of specific versions; only between 24%{\textendash}40% provide source code). Cites to publications are particularly poor at providing version information, whereas informal mentions are particularly poor at providing crediting information. We provide recommendations to improve the practice of software citation, highlighting recent nascent efforts. Software plays an increasingly great role in scientific practice; it deserves a clear and useful place in scholarly communication.},
url = {http://doi.wiley.com/10.1002/asi.23538},
local-url = {file://localhost/Users/Kyle/Dropbox/Library.papers3/Files/35/35777581-6B58-4423-A744-4E4D6DBD0473.pdf},
file = {{35777581-6B58-4423-A744-4E4D6DBD0473.pdf:/Users/Kyle/Dropbox/Library.papers3/Files/35/35777581-6B58-4423-A744-4E4D6DBD0473.pdf:application/pdf;35777581-6B58-4423-A744-4E4D6DBD0473.pdf:/Users/Kyle/Dropbox/Library.papers3/Files/35/35777581-6B58-4423-A744-4E4D6DBD0473.pdf:application/pdf}},
